{
    "version": 2,
    "functions": {
        "api/index.js": {
            "maxDuration": 120,
            "memory": 1024
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}